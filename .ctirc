{
    "spinnerStream": "stdout",
    "progressStream": "stdout",
    "reasonerStream": "stderr",
    "options": [
        {
            "mode": "create",
            "project": "tsconfig.json",
            "exportFilename": "index.ts",
            "useSemicolon": true,
            "useBanner": false,
            "useTimestamp": false,
            "quote": "'",
            "directive": "",
            "fileExt": "none",
            "overwrite": true,
            "backup": false,
            "generationStyle": "auto",
            "include": [
                "**/*.ts"
            ],
            "exclude": [
                "**/__mocks__/*"
            ],
            "skipEmptyDir": true,
            "startFrom": "./lib",
            "output": "./dist",
            "removeBackup": false,
            "forceYes": false
        }
    ]
}
